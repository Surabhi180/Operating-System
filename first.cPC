#include<stdio.h>
#include<unistd.h>
#include<stdlib.h>
#include<sys/type.h>
int main ()
{
pid_t p=fork();
if(p=0)}
printf("I am the child process and with id:%d,getpid());
printf("my parent id is:%d",getpid());
}
else if(p>0)
{
printf("I am a parent process with id:%d",getpid());
printf("my child process is %d",p);
}if(p<0){
printf("fork fail");
exit(1);
}
printf("hello all,my process id is %d \n",getpid());
}
